profile with_monitor {
    # to get the correct output name: WAYLAND_DEBUG=1 kanshi - and copy paste the make()
    output "Unknown Sceptre Z27 0x00000000" mode 3840x2160@60.000Hz scale 1.6 position 1920,150
    output eDP-1 mode 3840x2400@59.994Hz scale 2.3 position 4318,0
    exec notify-send "applying laptop + 1 monitor"
  }

profile laptop {
    output eDP-1 mode 3840x2400@59.994Hz scale 1.8
    exec notify-send "applying laptop screen only"
  }
